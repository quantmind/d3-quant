{
  "preset": "ts-jest",
  "rootDir": ".",
  "automock": false,
  "testEnvironment": "jsdom",
  "testMatch": ["<rootDir>/tests/*.test.js"],
  "coverageReporters": ["json", "html", "lcov", "text", "text-summary"],
  "coveragePathIgnorePatterns": ["node_modules", "dist", ".storybook"],
  "coverageThreshold": {
    "global": {
      "branches": 10,
      "functions": 10,
      "lines": 10,
      "statements": 10
    }
  }
}
